{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/index.js"
    }
  ],
  "crons": [
    {
      "path": "/cron/nico-ranking",
      "schedule": "0 9,18,23 * * *"
    },
    {
      "path": "/cron/vieshow-coming",
      "schedule": "0 16 * * *"
    }
  ]
}